{
  "compileOnSave": false,
  "compilerOptions": {
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "jsx": "react",
    "allowJs": false,
    "noEmitOnError": false,
    "pretty": true,
    "removeComments": true,
    "target": "ES5"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "_deprecated"
  ],
  "paths": {
    "lodash/*": [
      "node_modules/@types/lodash-es/*"
    ]
  }
}
